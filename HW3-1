        "x = int(input())\n",
        "y = int(input())\n",
        "\n",
        "if f(x=x,y=y) & f(x=y,y=x):\n",
        "  print(\"{0}와{1}는 같은 수이다.\".format(x,y))\n",
        "elif f(x=x,y=y):\n",
        "  print(\"{0}는{1}의 약수이지만,{1}가 아니다.\".format(x,y))\n",
