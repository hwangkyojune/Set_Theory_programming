        "def f(x = 1, y = 1):\n",
        "  if y % x == 0:\n",
        "    return 1\n",
        "  else:\n",
        "    return 0"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "x = int(input())\n",
        "y = int(input())\n",
        "\n",
        "if f(x=x,y=y) & f(x=y,y=x):\n",
        "  print(\"{0}와{1}는 같은 수이다.\".format(x,y))\n",
        "elif f(x=x,y=y):\n",
        "  print(\"{0}는{1}의 약수이지만,{1}가 아니다.\".format(x,y))\n",
        "elif f(x=y,y=x):\n",
        "  print(\"{1}는{0}의 약수이지만,{0}가 아니다.\".format(x,y))\n",
        "else:\n",
        "  print(\"{0}와{1}는 서로 약수가 아니다.\".format(x,y))"
